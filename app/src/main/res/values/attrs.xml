<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DrawableRadioButton">
        <attr name="drawableSize" format="dimension"/>
        <attr name="drawableTop" format="reference"/>
        <attr name="drawableLeft" format="reference"/>
        <attr name="drawableRight" format="reference"/>
        <attr name="drawableBottom" format="reference"/>
        <attr name="drawableTopSelected" format="reference"/>
        <attr name="drawableLeftSelected" format="reference"/>
        <attr name="drawableRightSelected" format="reference"/>
        <attr name="drawableBottomSelected" format="reference"/>
    </declare-styleable>

    <declare-styleable name="TLCycleRatioView">
        <attr name="backgroundColor" format="color|reference"></attr>
        <attr name="foreColor" format="color|reference"></attr>
        <attr name="innerTextColor" format="color|reference"></attr>
    </declare-styleable>

    <declare-styleable name="TLIndexView">

        <attr name="bgColor" format="color|reference"></attr>
        <attr name="textColor" format="color|reference"></attr>
        <attr name="selectTextColor" format="color|reference"></attr>
        <attr name="textSize" format="dimension"></attr>

    </declare-styleable>
</resources>